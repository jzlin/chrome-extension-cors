{
  "name" : "CORS",
  "version" : "1.0.0",
  "manifest_version": 2,
  "description" : "Cross-origin resource sharing",
  "background" : {
    "scripts" : [
      "background.js"
    ],
    "persistent" : false
  },
  "browser_action" :
  {
    "default_icon" : {
      "19" : "cors_128.png",
      "38" : "cors_128.png"
    },
    "default_title" : "Cross-origin resource sharing"
  },
  "content_security_policy" : "default-src 'self'; style-src 'self' 'unsafe-inline'; script-src 'self' 'unsafe-eval'; img-src *; connect-src *;",
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts" : [
    {
      "matches" : [
        "http://*/*",
      	"https://*/*"
      ],
      "js" : [
        "jquery.js",
        "contentscript.js"
      ],
      "run_at" : "document_idle",
      "all_frames" : false
    }
  ],
  "icons" : {
    "16" : "cors_128.png",
    "48" : "cors_128.png",
    "128" : "cors_128.png"
  },
  "web_accessible_resources": [
  ]
}
